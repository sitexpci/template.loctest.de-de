<!DOCTYPE html>
{% assign contentLocale = page.locale | downcase %}
{% include htmltag.html %}
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta name="ms.pagetype" content="hubpage" />
    <meta name="ms.sitename" content="DOCS" />
    {{ metadata }}
    {% if page._op_canonicalUrl != null %}<link href="{{ page._op_canonicalUrl }}" rel="canonical">{% endif %}
    <title>{{ page.title }}</title>
    {% style css\cea131a85c4d3ac3b1e4.hubpage.css %}
  </head>
  <body>
    {% include header.html %}
    <div ms.pgarea="body">
      {{ content }}
    </div>
    <div id="footerWrapper"></div>
    {% js javascript\cea131a85c4d3ac3b1e4.hubpage.js %}
  </body>
</html>
